# Clash SpeedTest Docker Environment Configuration
# Copy this file to .env and adjust values as needed

# Timezone
TZ=Asia/Shanghai

# Ports
BACKEND_PORT=8080
FRONTEND_PORT=3000
DOWNLOAD_PORT=8081

# API Configuration
VITE_API_URL=http://backend:8080
LOG_LEVEL=info

# Paths
CONFIG_PATH=./backend/configs
OUTPUT_PATH=./backend/output

# Network
NETWORK_SUBNET=172.20.0.0/24

# Docker Registry (for production)
DOCKER_REGISTRY=ghcr.io
IMAGE_PREFIX=clash-speedtest
VERSION=latest

# Build Options
ENABLE_UPX=false